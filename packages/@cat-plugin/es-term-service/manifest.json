{
  "$schema": "../../plugin-core/manifest-schema.json",
  "id": "es-term-service",
  "entry": "dist/index.js",
  "version": "0.1.0",
  "iconURL": "https://unocss.dev/logo.svg",
  "services": [
    {
      "id": "ES",
      "type": "TERM_SERVICE"
    }
  ],
  "tags": ["term-service"],
  "config": {
    "type": "object",
    "properties": {
      "connection": {
        "type": "object",
        "properties": {
          "url": {
            "type": "string",
            "format": "url",
            "default": "http://localhost:9200/"
          },
          "username": {
            "type": "string",
            "default": "elastic"
          },
          "password": {
            "type": "string",
            "default": "elastic",
            "x-secret": true
          }
        }
      }
    }
  }
}
