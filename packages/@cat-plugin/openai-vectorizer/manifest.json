{
  "$schema": "../../plugin-core/manifest-schema.json",
  "id": "openai-vectorizer",
  "entry": "dist/index.js",
  "version": "0.1.0",
  "iconURL": "https://unocss.dev/logo.svg",
  "services": [
    {
      "id": "openai",
      "type": "TEXT_VECTORIZER"
    }
  ],
  "config": {
    "type": "object",
    "properties": {
      "model-id": {
        "type": "string",
        "default": "snowflake-arctic-embed2"
      },
      "url": {
        "type": "string",
        "format": "url",
        "default": "http://localhost:11434"
      },
      "path": {
        "type": "string",
        "format": "url",
        "default": "/v1/embeddings"
      }
    }
  }
}
