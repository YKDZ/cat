    "@types/unist": "^3.0.3"
  },
  "peerDependencies": {
    "@cat/db": "workspace:*",
    "@cat/plugin-core": "workspace:*",
    "@cat/shared": "workspace:*",
    "lodash-es": "^4.17.21",
    "zod": "^4.1.12"
