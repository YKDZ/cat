{
  "name": "@cat/shared",
  "version": "0.1.0",
  "keywords": [
    "cat",
    "plugins",
    "types",
    "typescript"
  ],
  "license": "MIT",
  "files": [
    "dist",
    "package.json"
  ],
  "type": "module",
  "main": "./dist/index.js",
