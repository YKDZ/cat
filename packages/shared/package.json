{
  "name": "@cat/shared",
  "version": "0.1.0",
  "license": "MIT",
  "keywords": [
    "plugins",
    "typescript",
    "types",
    "cat"
  ],
  "type": "module",
  "main": "./dist/index.cjs.js",
  "module": "./dist/index.es.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.es.js",
      "require": "./dist/index.cjs.js"
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "vite build",
    "test": "vitest --typecheck",
    "lint": "eslint . --fix",
    "format": "prettier --write .",
    "format:check": "prettier --write .",
    "clean": "rimraf dist"
  },
  "devDependencies": {
    "@cat/eslint-config": "workspace:*",
    "@types/node": "^24.2.0",
    "tsx": "^4.20.3",
    "typescript": "^5.9.2",
    "vite": "^7.0.6",
    "vite-plugin-dts": "^4.5.4",
    "zod": "^4.0.14"
  },
  "dependencies": {
    "pino": "^9.7.0"
  }
}