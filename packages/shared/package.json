{
  "name": "@cat/shared",
  "version": "0.1.0",
  "license": "MIT",
  "keywords": [
    "plugins",
    "typescript",
    "types",
    "cat"
  ],
  "type": "module",
  "main": "./dist/index.cjs.js",
  "module": "./dist/index.es.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.es.js",
      "require": "./dist/index.cjs.js"
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "nx vite:build",
    "test": "nx vite:test --typecheck",
    "lint": "eslint . --fix",
    "format": "prettier --write .",
    "format:check": "prettier --write .",
    "clean": "rimraf dist"
  },
  "dependencies": {
    "pino": "^9.8.0"
  },
  "devDependencies": {
    "@cat/eslint-config": "workspace:*",
    "zod": "^4.0.17"
  }
}
