    },
    "./schema/drizzle/vector": {
      "types": "./dist/schema/drizzle/vector.d.ts",
      "import": "./dist/schema/drizzle/vector.js"
    },
    "./schema/drizzle/enum": {
      "types": "./dist/schema/drizzle/enum.d.ts",
      "import": "./dist/schema/drizzle/enum.js",
      "require": "./dist/schema/drizzle/enum.cjs"
    }
  },
  "scripts": {
    "build": "vite build",
    "lint": "oxlint --type-aware",
    "lint:fix": "oxlint --fix --type-aware",
    "format": "oxfmt",
    "format:check": "oxfmt --check"
  },
  "devDependencies": {
    "@cat/eslint-config": "workspace:*",
    "unplugin-dts": "1.0.0-beta.6",
    "vite": "^7.3.0"
