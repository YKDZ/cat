{
  "name": "@cat/plugin-core",
  "version": "0.1.0",
  "keywords": [
    "cat",
    "plugins",
    "types",
    "typescript"
  ],
  "license": "MIT",
  "files": [
    "dist",
    "package.json"
  ],
  "type": "module",
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js"
    },
    "./client": {
      "types": "./dist/client/index.d.ts",
      "import": "./dist/client.js"
    },
    "./term-services": {
      "types": "./dist/services/term-services.d.ts",
      "import": "./dist/term-services.js"
    }
  },
  "scripts": {
    "build": "vite build",
    "lint": "oxlint --type-aware",
    "lint:fix": "oxlint --fix --type-aware",
    "format": "oxfmt",
    "format:check": "oxfmt --check"
  },
  "devDependencies": {
    "@cat/eslint-config": "workspace:*",
    "@types/lodash-es": "^4.17.12",
    "hono": "^4.11.10",
    "unplugin-dts": "1.0.0-beta.6",
    "vite": "^7.3.1"
  },
  "peerDependencies": {
    "@cat/db": "workspace:*",
    "@cat/shared": "workspace:*",
    "@types/lodash-es": "^4.17.12",
    "dompurify": "^3.3.1",
    "lodash-es": "^4.17.21",
    "vue": "^3.5.26",
    "zod": "^4.3.5"
  }
}
