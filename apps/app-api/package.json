{
  "name": "@cat/app-api",
  "version": "0.1.0",
  "keywords": [
    "cat",
    "plugins",
    "types",
    "typescript"
  ],
  "license": "MIT",
  "files": [
    "dist",
    "package.json"
  ],
  "type": "module",
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js"
    },
    "./app": {
      "types": "./dist/app.d.ts",
      "import": "./dist/app.js"
    },
    "./orpc/router": {
      "types": "./dist/orpc/router.d.ts",
      "import": "./dist/orpc/router.js"
    },
    "./context": {
      "types": "./dist/utils/context.d.ts",
      "import": "./dist/context.js"
    }
  },
  "scripts": {
    "build": "vite build",
    "lint": "oxlint --type-aware",
    "lint:fix": "oxlint --fix --type-aware",
    "format": "oxfmt",
    "format:check": "oxfmt --check",
    "typecheck": "tsc --noEmit -p tsconfig.lib.json"
  },
  "devDependencies": {
    "@cat/eslint-config": "workspace:*",
    "unplugin-dts": "1.0.0-beta.6",
    "vite": "^7.3.1"
  },
  "peerDependencies": {
    "@cat/app-server-shared": "workspace:*",
    "@cat/app-workers": "workspace:*",
    "@cat/db": "workspace:*",
    "@cat/plugin-core": "workspace:*",
    "@cat/shared": "workspace:*",
    "@hono/node-ws": "^1.3.0",
    "@orpc/client": "^1.13.2",
    "@orpc/experimental-pino": "^1.13.2",
    "@orpc/server": "^1.13.2",
    "hono": "^4.11.4",
    "pino": "^10.1.0",
    "telefunc": "^0.2.17",
    "vike": "^0.4.240",
    "zod": "^4.3.5"
  }
}
