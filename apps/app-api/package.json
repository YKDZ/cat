{
  "name": "@cat/app-api",
  "version": "0.1.0",
  "license": "MIT",
  "keywords": [
    "plugins",
    "typescript",
    "types",
    "cat"
  ],
  "type": "module",
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js"
    },
    "./trpc": {
      "types": "./dist/trpc/index.d.ts",
      "import": "./dist/trpc.js"
    },
    "./trpc/client": {
      "types": "./dist/trpc/client.d.ts",
      "import": "./dist/trpc/client.js"
    },
    "./trpc/sscClient": {
      "types": "./dist/trpc/sscClient.d.ts",
      "import": "./dist/trpc/sscClient.js"
    }
  },
  "files": [
    "dist",
    "package.json"
  ],
  "scripts": {
    "build": "vite build",
    "lint": "oxlint --fix",
    "format": "prettier --write .",
    "typecheck": "tsc --noEmit -p tsconfig.lib.json"
  },
  "devDependencies": {
    "@cat/eslint-config": "workspace:*"
  },
  "peerDependencies": {
    "@cat/plugin-core": "workspace:*",
    "@cat/db": "workspace:*",
    "@cat/shared": "workspace:*",
    "@cat/app-server-shared": "workspace:*",
    "@cat/app-workers": "workspace:*",
    "zod": "^4.1.9",
    "@trpc/client": "^11.5.1",
    "@trpc/server": "^11.5.1",
    "vike": "^0.4.240"
  }
}
