  "prisma": {
    "seed": "tsx prisma/seed-docker.ts"
  },
  "engines": {
    "node": ">=22.0.0"
  },
  "scripts": {
    "dev": "cross-env NODE_ENV=development vike dev",
    "build": "vike build",
    "preview": "cross-env NODE_ENV=production node dist/server/index.js",
    "start": "cross-env NODE_ENV=production node dist/server/index.js",
    "lint": "eslint ./src --fix",
    "format": "prettier --write .",
    "docker:build": "docker buildx build ../../ -f Dockerfile -t ykdz/cat",
    "docker:push": "docker push ykdz/cat",
    "docker-entrypoint": "prisma migrate deploy && prisma db seed && node dist/server/index.js"
  },
